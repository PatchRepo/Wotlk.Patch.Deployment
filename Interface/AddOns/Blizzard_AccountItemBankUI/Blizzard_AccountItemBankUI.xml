	<Button name="AccountItemBankItemButtonTemplate" inherits="ItemButtonTemplate" virtual="true">
		<Frames>
			<Cooldown name="$parentCooldown" inherits="CooldownFrameTemplate"/>
		</Frames>
		<Scripts>
			<OnClick>
        local type, money = GetCursorInfo();
        if ( button == "RightButton" ) then
						AutoStoreAccountItemBankItem(GetCurrentAccountItemBankTab(), self:GetID());
				end
			</OnClick>
			<OnLoad>
				AccountItemBankItemButton_OnLoad(self);
			</OnLoad>
			<OnEnter>
				AccountItemBankItemButton_OnEnter(self, motion);
			</OnEnter>
			<OnLeave>
				self.updateTooltip = nil;
				GameTooltip:Hide();
				ResetCursor();
			</OnLeave>
			<OnHide>
				if ( self.hasStackSplit and (self.hasStackSplit == 1) ) then
					StackSplitFrame:Hide();
				end
			</OnHide>
      <OnUpdate>
				--AccountItemBankItemButton_OnUpdate(self, elapsed);
			</OnUpdate>
			<OnEvent>
				if ( GameTooltip:IsOwned(self) ) then
					AccountItemBankItemButton_OnEnter(self);
				end
			</OnEvent>
		</Scripts>
	</Button>
